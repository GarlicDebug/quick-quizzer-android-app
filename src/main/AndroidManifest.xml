<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.andrews.reise.quickquizzer">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/summerscales_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/summerscales_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".QQMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuestionDetailsActivity" >
        </activity>
        <activity android:name=".QuestionListActivity" >
        </activity>
        <activity android:name=".QuizzingScreenActivity" >
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>